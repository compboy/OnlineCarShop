<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Cyber Cars</title>
</head>
<body>
    <%- include('partials/navbar', {
        nav: {
            addCar: '/admin/car',
            addMnfctr: '/admin/manufacturer',
            cars: '/admin/car/update',
            manufacturers: '/admin/manufacturer/update',
            search: '/admin/search',
            users: '/admin/customerDetails',
            messages: '/admin/message',
            status: '/admin/carstatus',
            logout: '/admin/logout'
        },
        active: 'search'
    }) %>
    <div class="container">
        <div class="table-data">
            <h2>Search Results</h2>
            <table>
                <tr>
                    <th>Manufacturer</th>
                    <th>Model</th>
                    <th>Color</th>
                    <th>Price</th>
                </tr>
                <% for(var i = 0; i < data.length; i++) { %>
                    <tr>
                    <td> <%= data[i].name %></td>
                   <td><a href="/admin/car/update/<%=data[i].carID%>"><%= data[i].model%></a> </td>
                   <td> <%= data[i].color %> </td>
                    <td> <%= data[i].price %></td>
                    </tr>
                <% } %>
        </table>
        </div>
    </div>
    
</body>
<script>
    function ConfirmDelete()
    {
      var x = confirm("Are you sure you want to delete?");
      if (x)
          return true;
      else
        return false;
    }
</script> 
</html>